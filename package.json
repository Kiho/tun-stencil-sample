{
  "name": "tun-stencil-sample",
  "private": false,
  "version": "0.0.1",
  "description": "Stencil Elements Sample",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "module": "dist/esm/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --prerender",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "@stencil/core": "^0.12.4",
    "@stencil/router": "^0.2.6",
    "bulma": "^0.7.1"
  },
  "devDependencies": {
    "@stencil/sass": "^0.1.0",
    "@types/jest": "^23.3.1",
    "jest": "^23.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AngelMunoz/SuperEasyBoringForms.git"
  },
  "author": "Angel Munoz",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
